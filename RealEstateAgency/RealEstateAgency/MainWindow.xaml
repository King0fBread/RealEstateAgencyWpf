<Window x:Class="RealEstateAgency.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RealEstateAgency"
        mc:Ignorable="d"
        Title="RealEstateAgency" Height="500" Width="950" MinHeight="495" MinWidth="930" MaxHeight="530" MaxWidth="970" ResizeMode="CanMinimize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Background="#FF2286FE" Orientation="Horizontal" HorizontalAlignment="Center" Width="950">
            <Image Source="/Resources/HouseLogo.png" Height="60" HorizontalAlignment="Left"/>
            <Label Content="House23" VerticalAlignment="Center" Foreground="White" FontSize="36" Margin="330 0"/>
        </StackPanel>
        <Frame x:Name="mainFrame" Grid.Row="1"></Frame>
    </Grid>
</Window>
